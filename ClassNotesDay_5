List<Integer> myIntList=new List<Integer>{1,2,3,4,5};
    system.debug('Elements of this List' +myIntList);
    
for(Integer myInteger:myIntList){
    Integer newInt=myInteger*5;
    system.debug('New Integer is ' +newInt);
}
